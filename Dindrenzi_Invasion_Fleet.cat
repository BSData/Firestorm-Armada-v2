    <profile id="bdb1eb47-8b94-03d6-d45c-05d03ae3762f" name="Retarius" hidden="false" profileTypeId="601947e0-0125-7417-9d3a-5e3caf6e031d">
      <profiles/>
      <rules/>
      <infoLinks/>
