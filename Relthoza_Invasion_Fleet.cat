    <profile id="c546f64a-76a0-3616-8c3d-5fa1dec2fe51" name="Widow_Weapon_I" page="0" hidden="false" profileTypeId="b43e-2222-4230-3aac" profileTypeName="Weapon">
      <profiles/>
      <rules/>
      <infoLinks/>
      <modifiers/>
